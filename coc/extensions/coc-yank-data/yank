	TestToto = {} --class
	
	    function TestToto:setUp(CN)
				if CN == nil then
					print("function without Channel")
				else
					print("channel is ", CN)
				end
				print("This is SetUp")
	        -- set up tests
	        self.a = 1
	        self.s = 'hop' 
	        self.t1 = {1,2,3}
	        self.t2 = {one=1,two=2,three=3}
	        self.t3 = {1,2,three=3}
	    end
	
			function TestToto:tearDown(CN)
				if CN == nil then
					print("function without Channel")
				else
					print("channel is ", CN)
				end
				print("This is tearDown")
			end
	--[==[
	    function TestToto:test1_withFailure()
	        -- print( "some stuff test 1" )
